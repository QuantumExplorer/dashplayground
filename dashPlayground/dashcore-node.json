{
  "network": "testnet",
  "port": 3001,
  "services": [
    "dashd",
    "web",
    "@dashevo/insight-api"
  ],
  "servicesConfig": {
    "dashd": {
      "connect": [
        {
          "rpchost": "127.0.0.1",
          "rpcport": 19998,
          "rpcuser": "dash",
          "rpcpassword": [RPC_PASSWORD],
          "zmqpubrawtx": "tcp://dashcore:30003",
          "zmqpubhashblock": "tcp://dashcore:30003"
        }
      ]
    },
    "@dashevo/insight-api": {
      "disableRateLimiter": true
    }
  }
}
